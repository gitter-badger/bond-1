{
  "name": "Bitbucket-Redmine-Sync",
  "description": "My first Chrome App.",
  "version": "0.0.4",
  "manifest_version": 2,
  "icons": {
    "16": "icons/calculator-16.png",
    "128": "icons/calculator-128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_title": "go,go,go",
    "default_icon": "icons/calculator-128.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}
